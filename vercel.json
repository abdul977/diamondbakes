{
  "routes": [
    { 
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    { 
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    { 
      "src": "/(.+\\.(?:ico|png|jpg|jpeg|webp|svg|gif|tiff|js|css|woff|woff2)$)",
      "dest": "/$1",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    { 
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": { "cache-control": "public,max-age=0,must-revalidate" }
    }
  ]
}